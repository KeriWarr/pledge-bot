{
  "name": "pledge-bot",
  "version": "0.1.0",
  "description": "A slack bot for making wagers with your friends",
  "main": "index.js",
  "scripts": {
    "dev": "babel src -w -d compiled & nodemon -w compiled index.js",
    "build": "babel src --out-dir compiled",
    "run": "node index.js",
    "start": "npm run build; npm run run",
    "lint": "eslint . --ext .js --ext .jsx || true",
    "lint-with-errors": "eslint . --ext .js --ext .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeriWarr/pledge-bot.git"
  },
  "keywords": [
    "slack",
    "splitwise",
    "botstrap",
    "wager"
  ],
  "author": "Keri Warr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KeriWarr/pledge-bot/issues"
  },
  "homepage": "https://github.com/KeriWarr/pledge-bot#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "botstrap": "^0.15.4",
    "lodash": "^4.16.4",
    "node-fetch": "^1.6.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "nodemon": "^1.11.0"
  }
}
